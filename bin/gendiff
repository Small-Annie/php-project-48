#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

$doc = <<<'DOCOPT'
Generate diff

Usage:
  gendiff (-h|--help)
  gendiff (-v|--version)

Options:
  -h --help                     Show this screen
  -v --version                  Show version
DOCOPT;

$args = Docopt::handle($doc, ['version' => '1.0']);
